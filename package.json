{
    "name": "@dexma/ui-components",
    "private": false,
    "version": "7.0.13",
    "main": "./dist/ui-components.umd.js",
    "module": "./dist/ui-components.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/ui-components.es.js",
            "require": "./dist/ui-components.umd.js"
        }
    },
    "engines": {
        "node": "18"
    },
    "scripts": {
        "build": "npm run build-icons && tsc && vite build",
        "build-storybook": "npm run build-icons && storybook build",
        "build-icons": "node scripts/buildIconConfig.js",
        "coverage": "vitest run --coverage && serve coverage",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix",
        "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
        "test": "npm run build-icons && vitest",
        "test:ui": "npm run build-icons && vitest --ui --config vitest-ui.config.ts",
        "prettier": "prettier --write \"{src,test}/**/*.+(ts|tsx|json|mdx)\"",
        "prepare": "husky install",
        "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
        "semantic-release": "semantic-release",
        "storybook": "npm run build-icons && storybook dev -p 6006"
    },
    "publishConfig": {
        "tag": "latest"
    },
    "release": {
        "branch": "master",
        "tagFormat": "v${version}",
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/npm",
            "@semantic-release/github",
            "@semantic-release/changelog",
            [
                "@semantic-release/git",
                {
                    "message": "release (${nextRelease.version})"
                }
            ]
        ]
    },
    "dependencies": {
        "@storybook/cli": "^8.1.6",
        "@types/hoist-non-react-statics": "^3.3.5",
        "@types/lodash": "^4.14.202",
        "@xmldom/xmldom": "^0.8.10",
        "highcharts-react-official": "^3.1.0",
        "hoist-non-react-statics": "^3.3.2",
        "react-color": "^2.19.3",
        "react-copy-to-clipboard": "^5.1.0"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^8.1.6",
        "@storybook/addon-interactions": "^8.1.6",
        "@storybook/addon-links": "^8.1.6",
        "@storybook/addon-storysource": "^8.1.6",
        "@storybook/react": "^8.1.6",
        "@storybook/react-vite": "^8.1.6",
        "@storybook/testing-library": "0.2.0",
        "@testing-library/jest-dom": "6.0.1",
        "@testing-library/react": "15.0.7",
        "@types/node": "20.4.9",
        "@types/prop-types": "^15.7.8",
        "@types/react": "18.2.25",
        "@types/react-color": "^3.0.12",
        "@types/react-copy-to-clipboard": "^5.0.7",
        "@types/react-dom": "18.2.7",
        "@typescript-eslint/eslint-plugin": "6.4.1",
        "@typescript-eslint/parser": "6.4.1",
        "@vitejs/plugin-react": "4.0.4",
        "@vitest/coverage-v8": "0.34.2",
        "@vitest/ui": "0.34.2",
        "eslint": "8.47.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-config-standard-with-typescript": "38.0.0",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-n": "16.0.2",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "0.6.13",
        "husky": "8.0.3",
        "jsdom": "22.1.0",
        "json": "11.0.0",
        "lint-staged": "13.2.3",
        "prettier": "3.0.1",
        "prop-types": "15.8.1",
        "storybook": "^8.1.6",
        "typescript": "5.1.6",
        "vite": "^4.5.3",
        "vite-plugin-dts": "3.5.1",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "0.34.1"
    },
    "peerDependencies": {
        "antd": "5.5.1",
        "highcharts": "^11.3.0",
        "moment": "^2.30.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "6.0.7"
    },
    "volta": {
        "node": "18.17.0",
        "npm": "9.6.7"
    },
    "files": [
        "dist"
    ],
    "lint-staged": {
        "*.{ts,tsx,js,jsx,json,css,md}": [
            "prettier -w"
        ]
    }
}
